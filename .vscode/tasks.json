{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "C:/ProgramData/chocolatey/bin/godot.exe",
            "type": "process",
            "args": [
                "--build-solutions",
                "--path",
                "${workspaceRoot}",
                "--no-window",
                "-q"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "type": "msbuild",
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "label": "Build: solution",
            "detail": "Build the Themed Horror Jam 5.sln solution using dotnet build"
        }
    ]
}